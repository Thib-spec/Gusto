    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.17.3",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "chart.js": "^2.9.4",
    "deepcopy": "^2.1.0",
    "dotenv": "^10.0.0",
    "i18next": "^21.5.4",
    "i18next-http-backend": "^1.3.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^17.0.2",
    "react-dropdown": "^1.9.2",
    "react-i18next": "^11.14.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "sass": "^1.43.4",
    "styled-components": "^5.3.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "env-cmd": "^10.1.0",
    "json-server": "^0.17.0",
    "nams": "^1.0.1",
    "nodemon": "^2.0.15",
    "react-scripts": "^3.4.4"
  },
  "scripts": {
    "start": "npm run dev:nams",
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "json-server-start:development": "env-cmd -f .env.development.local nodemon --watch ./dev/json-server -e json,js ./dev/json-server/server.js",
    "nams-start:development": "env-cmd -f .env.development.local nodemon --watch ./dev/nams -e json,js ./dev/nams/server.js",
    "dev:json-server": "concurrently 'npm:json-server-start:development' 'npm:react-start'",
    "dev:nams": "concurrently 'npm:nams-start:development' 'npm:react-start'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "react",
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
